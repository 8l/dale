(import cstdio) 
(import macros) 

(using-namespace std.macros
(def print-arg-types
  (macro intern (fn)
    (let ((n int (arity mcontext fn))
          (i int 0))
      (for true (< i n) (incv i)
        (print (input-type mcontext fn i))
        (printf "\n")))
    (mnfv mcontext "0")))
)

(def main
  (fn extern-c int (void)
    (print-arg-types malloc)
    (print-arg-types printf)
    (print-arg-types memcpy)
    0))

